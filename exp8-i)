#include <iostream>
using namespace std;

class MyString {
private:
    string str;

public:
    MyString(string s = "") : str(s) {}

    MyString operator + (const MyString &other) const {
        return MyString(str + other.str);
    }

    void display() const {
        cout << str << endl;
    }
};

int main() {
    MyString s1("xyz");
    MyString s2("pqr");
    MyString s3 = s1 + s2;

    cout << "Concatenated string: ";
    s3.display();

    return 0;
}
output
Concatenated string: xyzpqr
