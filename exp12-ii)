#include <iostream>
#include <queue>
using namespace std;

int main() {
    queue<int> q;

    q.push(10);
    q.push(20);
    q.push(30);

    cout << "Front element: " << q.front() << endl;
    cout << "Back element: " << q.back() << endl;

    q.pop();  // removes front element

    cout << "\nAfter pop operation:" << endl;
    cout << "Front element: " << q.front() << endl;
    cout << "Queue size: " << q.size() << endl;

    if (q.empty())
        cout << "Queue is empty" << endl;
    else
        cout << "Queue is not empty" << endl;

    return 0;
}
output
Front element: 10
Back element: 30

After pop operation:
Front element: 20
Queue size: 2
Queue is not empty
